{
  "name": "firestore-storage",
  "version": "4.1.0",
  "description": "A typed wrapper around Firestore including a query-builder and an in-memory implementation for testing",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "keywords": [
    "firestore",
    "typescript",
    "memory",
    "querybuilder"
  ],
  "directories": {
    "test": "test",
    "lib": "src"
  },
  "files": [
    "src/lib",
    "dist/lib",
    "dist/test/rules",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=10.10.0"
  },
  "scripts": {
    "test": "yarn test:memory && yarn test:firestore",
    "test:firestore": "yarn test:memory --firestore",
    "test:memory": "NODE_ENV=test mocha dist/test",
    "build": "yarn clean && tsc --build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "firebase": "firebase",
    "fss": "fss"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:freshfox/firestore-storage.git"
  },
  "author": "Dominic Bartl",
  "license": "MIT",
  "homepage": "https://github.com/freshfox/firestore-storage",
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "ff-utils": "^1.3.1",
    "firestore-storage-core": "^4.1.0",
    "inversify": "^5.0.1",
    "lodash": "^4.17.20",
    "lodash.camelcase": "^4.3.0",
    "lodash.isplainobject": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@nestjs/microservices": "^7.4.4",
    "@nestjs/testing": "^7.4.4",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "firebase-functions-test": "^0.2.2",
    "firebase-tools": "^8.11.2",
    "mocha": "^8.1.3",
    "node-env-file": "^0.1.8",
    "should": "^13.2.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "firebase-admin": ">=8.8.0",
    "firebase-tools": ">=8.4.0"
  },
  "gitHead": "a1f1f0ddbc96854b263f30768da698b1111dae6e"
}
